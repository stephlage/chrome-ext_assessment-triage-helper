{
  "manifest_version": 2,
  "name": "Assessment Triage Helper",
  "version": "1.0",
  "description": "This extension helps to guide the triage process for assessment tickets.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
